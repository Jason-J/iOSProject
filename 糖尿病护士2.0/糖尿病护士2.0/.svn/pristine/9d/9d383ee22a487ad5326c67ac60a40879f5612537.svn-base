#import "AlarmClock.h"

@implementation AlarmClock
- (instancetype)init
{
    self = [super init];
    if (self) {
        _Monday = @"1";
        _Tuesday = @"1";
        _Wednesday = @"1";
        _Thursday = @"1";
        _Friday = @"1";
        _Saturday = @"1";
        _Sunday = @"1";
        _isOpen = @"0";
        _isShake = @"0";
    }
    return self;
}
-(void)encodeWithCoder:(NSCoder *)aCoder
{
    [aCoder encodeObject:self.Monday forKey:@"Monday"];
    [aCoder encodeObject:_Tuesday forKey:@"Tuesday"];
    [aCoder encodeObject:_Wednesday forKey:@"Wednesday"];
    [aCoder encodeObject:_Thursday forKey:@"Thursday"];
    [aCoder encodeObject:_Friday forKey:@"Friday"];
    [aCoder encodeObject:_Saturday forKey:@"Saturday"];
    [aCoder encodeObject:_Sunday forKey:@"Sunday"];
    [aCoder encodeObject:_voiceUrl forKey:@"voiceUrl"];
    [aCoder encodeObject:_isShake forKey:@"isShake"];
    [aCoder encodeObject:_isOpen forKey:@"isOpen"];
    [aCoder encodeObject:_remarks forKey:@"remarks"];
    [aCoder encodeObject:_time forKey:@"time"];
    [aCoder encodeObject:_repeat forKey:@"repeat"];
    
    
    [aCoder encodeObject:_checkProject forKey:@"checkProject"];
    [aCoder encodeObject:_lastCheckTime forKey:@"lastCheckTime"];
    [aCoder encodeObject:_checkCycle forKey:@"checkCycle"];
    [aCoder encodeObject:_familyArr forKey:@"familyArr"];
    [aCoder encodeObject:_allFamily forKey:@"allFamily"];
    [aCoder encodeObject:_alarmType forKey:@"alarmType"];
}
- (id)initWithCoder:(NSCoder *)aDecoder
{
    self = [super init];
    if (self) {
        _Monday = [aDecoder decodeObjectForKey:@"Monday"];
        _Tuesday = [aDecoder decodeObjectForKey:@"Tuesday"];
        _Wednesday = [aDecoder decodeObjectForKey:@"Wednesday"];
        _Thursday = [aDecoder decodeObjectForKey:@"Thursday"];
        _Friday = [aDecoder decodeObjectForKey:@"Friday"];
        _Saturday = [aDecoder decodeObjectForKey:@"Saturday"];
        _Sunday = [aDecoder decodeObjectForKey:@"Sunday"];
        _voiceUrl = [aDecoder decodeObjectForKey:@"voiceUrl"];
        _isShake = [aDecoder decodeObjectForKey:@"isShake"];
        _isOpen = [aDecoder decodeObjectForKey:@"isOpen"];
        _remarks = [aDecoder decodeObjectForKey:@"remarks"];
        _time = [aDecoder decodeObjectForKey:@"time"];
        _repeat = [aDecoder decodeObjectForKey:@"repeat"];
        
        _checkProject = [aDecoder decodeObjectForKey:@"checkProject"];
        _lastCheckTime = [aDecoder decodeObjectForKey:@"lastCheckTime"];
        _checkCycle = [aDecoder decodeObjectForKey:@"checkCycle"];
        _familyArr = [aDecoder decodeObjectForKey:@"familyArr"];
        _allFamily = [aDecoder decodeObjectForKey:@"allFamily"];
        _alarmType = [aDecoder decodeObjectForKey:@"alarmType"];
    
    }
    return self;
}
@end
