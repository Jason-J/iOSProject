    [aCoder encodeObject:_userAccount forKey:@"userAccount"];
    [aCoder encodeObject:_userPassword forKey:@"userPassword"];
    [aCoder encodeObject:_realname forKey:@"realname"];
    [aCoder encodeObject:_username forKey:@"username"];
    [aCoder encodeObject:_gender forKey:@"gender"];
    [aCoder encodeObject:_resideprovince forKey:@"resideprovince"];
    [aCoder encodeObject:_sightml forKey:@"sightml"];
    [aCoder encodeObject:_field1 forKey:@"field1"];
    [aCoder encodeObject:_field2 forKey:@"field2"];
    [aCoder encodeObject:_addtime forKey:@"addtime"];
    [aCoder encodeObject:_images forKey:@"images"];
    [aCoder encodeObject:_location forKey:@"location"];
    [aCoder encodeObject:_num forKey:@"num"];
    [aCoder encodeObject:_cookiepre forKey:@"cookiepre"];
    [aCoder encodeObject:_email forKey:@"email"];
    [aCoder encodeObject:_mobile forKey:@"mobile"];
