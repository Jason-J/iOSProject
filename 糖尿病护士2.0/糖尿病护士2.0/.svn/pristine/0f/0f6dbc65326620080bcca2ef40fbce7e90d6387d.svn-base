#import <UIKit/UIKit.h>

typedef void(^DATEBLOCK)(NSString *dateStr);
typedef void(^TWODATEBLOCK)(UIDatePicker *);

typedef void(^HIDDENVIEW)(void);
@interface MyDatePickerView : UIView
@property (nonatomic, strong) UIDatePicker *datePicker;
@property (nonatomic, copy) DATEBLOCK dateBlock;
@property (nonatomic, copy) TWODATEBLOCK datePickerBlock;
@property (nonatomic, copy) HIDDENVIEW hiddenBlock;
-(void)valueOfBlock:(DATEBLOCK)block;
-(void)valueOfDatePicker:(TWODATEBLOCK)twoBlock;
-(void)valueOfHiddenBlock:(HIDDENVIEW)hiddenBlock;
@end
