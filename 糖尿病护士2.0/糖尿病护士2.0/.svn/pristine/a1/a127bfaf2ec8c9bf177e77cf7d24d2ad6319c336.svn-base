@implementation MyDoctorTableViewCell (ConfigureForDoctor)

-(void)configureForModel:(Doctor *)doctor{
    self.doctorNameLabel.text = doctor.realname;
    self.titleLabel.text = doctor.title;
    self.hospitalLabel.text = doctor.hospital;
//    self.signLabel.text = doctor.speciality;
    [self.myImageView sd_setImageWithURL:[NSURL URLWithString:doctor.headimg] placeholderImage:[UIImage imageNamed:@"doctor_head-portrait"]];
    self.myImageView.layer.cornerRadius = 40;
    self.myImageView.layer.masksToBounds = YES;
    
}

@end