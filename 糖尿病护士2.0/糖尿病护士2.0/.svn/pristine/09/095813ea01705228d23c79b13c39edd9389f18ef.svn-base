@implementation FamousExpertCell (configureForExpert)

-(void)configureForExpert:(Doctor *)doctor{
    self.realname.text = doctor.realname;
    self.hospital.text = doctor.hospital;
    self.speciality.text = doctor.speciality;
    [self.headimg sd_setImageWithURL:[NSURL URLWithString:doctor.headimg] placeholderImage:[UIImage imageNamed:@"doctor_head-portrait"]];
    self.headimg.layer.cornerRadius = 55/2;
    self.headimg.layer.masksToBounds = YES;
}

@end