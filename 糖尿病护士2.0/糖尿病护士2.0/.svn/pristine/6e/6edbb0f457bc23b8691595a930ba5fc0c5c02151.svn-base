#import "CellForOnlyImage.h"
#import "UIImageView+WebCache.h"
@implementation CellForOnlyImage

- (void)awakeFromNib {
    // Initialization code
}

- (void)setSelected:(BOOL)selected animated:(BOOL)animated {
    [super setSelected:selected animated:animated];

    // Configure the view for the selected state
}
-(void)setQuestion:(Question *)question {
    if (_question!= question) {
        _question = question;
        NSLog(@"%@", _question.imageArr);
        for (int i = 0; i < _question.imageArr.count; i++) {
            UIImageView *imgeView = (UIImageView *)[self.contentView viewWithTag:10002 -i];
            imgeView.superview.hidden = NO;
            [imgeView sd_setImageWithURL:[NSURL URLWithString:_question.imageArr[i]]];
            NSLog(@"%@", imgeView.image);
        }
    }
}
- (IBAction)imageBtnAction:(UIButton *)sender
{
    UIImageView *imageView = (UIImageView *)[self.contentView viewWithTag:sender.tag - 10000];
    if (self.imageViewBlock) {
        self.imageViewBlock(imageView);
    }
}
-(void)valueOfImageView:(RETURNIMAGEVIEWBLOCK)imageViewBlock
{
    self.imageViewBlock = imageViewBlock;
}
@end
